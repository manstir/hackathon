@model Surrogat.Areas.CentralBank.ViewModels.BankSummaryViewModel
@{
    Layout = "~/Areas/CentralBank/Views/Shared/_CentralBankLayout.cshtml";
}
<div class="row">
    <div class="col-md-8">
        <div class="panel panel-default">
            <div class="panel-heading">
                <img width="48" src="@Url.Content("~/Images/ebill.png")" />EBills
            </div>
            <div class="panel-body">
                @{
                    Html.RenderAction("List", "Bill");
                }
            </div>
        </div>
    </div>
    <div class="col-md-2">
        @foreach (var i in @Model.IssuerIds)
        {
            Html.RenderAction("IssuerAccount", "Bank", new { IssuerId = @i });
        }
    </div>
    <div class="col-md-2">
        @foreach (var i in @Model.AcquirerIds)
        {
            Html.RenderAction("AcquirerAccount", "Bank", new { AcquirerId = @i });
        }
    </div>
</div>
